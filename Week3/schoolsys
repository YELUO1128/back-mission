CREATE DATABASE `schoolsys`CHARACTER SET utf8 COLLATE utf8_general_ci;
 SHOW DATABASES;
USE `schoolsys`;
SHOW FULL TABLES FROM `schoolsys` WHERE table_type = 'BASE TABLE';
 SHOW CHARSET;
 SHOW COLLATION;
 SHOW COLLATION;
 CREATE TABLE `schoolsys`.`student`( `id` VARCHAR(30) NOT NULL DEFAULT '0' COMMENT '学号', `sname` VARCHAR(30) NOT NULL COMMENT '姓名', `birthday` VARCHAR(30) NOT NULL COMMENT '出生日期', `sex` VARCHAR(10) COMMENT '性别', PRIMARY KEY (`id`) ) ENGINE=INNODB CHARSET=utf8 COLLATE=utf8_general_ci;
 SHOW FULL TABLES FROM `schoolsys` WHERE table_type = 'BASE TABLE';
 SHOW FULL TABLES FROM `schoolsys` WHERE table_type = 'BASE TABLE';
 SHOW CHARSET;
 SHOW TABLE STATUS FROM `schoolsys` LIKE 'student';
 SHOW CHARSET;
 SHOW FULL FIELDS FROM `schoolsys`.`student`;
 SHOW KEYS FROM `schoolsys`.`student`;
 SHOW COLLATION;
 SHOW COLLATION;
 SHOW COLLATION;
 CREATE TABLE `schoolsys`.`score`( `id` VARCHAR(30) NOT NULL COMMENT '学号', `lesson` VARCHAR(30) NOT NULL COMMENT '课程号', `score` INT(3) NOT NULL COMMENT '成绩', PRIMARY KEY (`id`) ) ENGINE=INNODB CHARSET=utf8 COLLATE=utf8_general_ci;
 SHOW FULL TABLES FROM `schoolsys` WHERE table_type = 'BASE TABLE';
 SHOW CHARSET;
 SHOW TABLE STATUS FROM `schoolsys` LIKE 'score';
 SHOW CHARSET;
 SHOW FULL FIELDS FROM `schoolsys`.`score`;
 SHOW KEYS FROM `schoolsys`.`score`;
 SHOW COLLATION;
SHOW COLLATION;
SHOW COLLATION;
 CREATE TABLE `schoolsys`.`course`( `courseid` VARCHAR(30) NOT NULL COMMENT '课程号', `coursename` VARCHAR(30) NOT NULL COMMENT '课程名', `teacherid` VARCHAR(30) NOT NULL COMMENT '教师号', PRIMARY KEY (`courseid`) ) ENGINE=INNODB CHARSET=utf8 COLLATE=utf8_general_ci;
 SHOW FULL TABLES FROM `schoolsys` WHERE table_type = 'BASE TABLE';
 SHOW CHARSET;
 SHOW TABLE STATUS FROM `schoolsys` LIKE 'course';
 SHOW CHARSET;
 SHOW FULL FIELDS FROM `schoolsys`.`course`;
 SHOW KEYS FROM `schoolsys`.`course`;
 SHOW COLLATION;
 SHOW COLLATION;
 SHOW COLLATION;
 CREATE TABLE `schoolsys`.`teacher`( `teacherid` VARCHAR(30) NOT NULL DEFAULT '0' COMMENT '教师号', `teachername` VARCHAR(30) NOT NULL COMMENT '教师姓名', PRIMARY KEY (`teacherid`) ) ENGINE=INNODB CHARSET=utf8 COLLATE=utf8_general_ci;
 SHOW TABLE STATUS FROM `schoolsys` LIKE 'teacher';
 SHOW CHARSET;
 SHOW FULL FIELDS FROM `schoolsys`.`teacher`;
 SHOW KEYS FROM `schoolsys`.`teacher`;
 SHOW COLLATION;
 SHOW FULL TABLES FROM `schoolsys` WHERE table_type = 'BASE TABLE';
